{
	"easycom": {
			"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "pages/index/index",
		// "aliasPath": "/", //对于h5端你必须在首页加上aliasPath并设置为/
		"style": {
			"navigationBarTitleText": "uni-ui基础项目"
		}
		}
	    ,{
            "path" : "pages/navigator/navigator",
            "style" : {}
        }
        ,{
            "path" : "pages/vuex/vuex",
            "style" : {}
        }
		,{
		    "path" : "pages/vuex/next",
		    "style" : {}
		}
        ,{
            "path" : "pages/flex/flex",
            "style" : {}
        }
		,{
            "path" : "pages/router/router",
            "style" : {}
        }
		,{
		    "path" : "pages/pay/pay",
		    "style" : {}
		}
        
        ,{
            "path" : "pages/mixins/mixins",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/uViewUI/class",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/uViewUI/components",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#007AFF",
		"backgroundColor": "#FFFFFF"
	}
}
